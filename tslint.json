{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "quotemark": {
      "options": ["single", "avoid-escape"]
    },
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "coma": [false],
    "no-console": false
  },
  "rulesDirectory": []
}
